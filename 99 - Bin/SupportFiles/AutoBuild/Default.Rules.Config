<?xml version="1.0" encoding="utf-8" ?>
<ruleConfiguration xmlns="urn:pdc:infosupport-autobuild-ruleconfiguration">
  <ruleSets>
    <ruleSet name="CodeStats">
      <rule name="CodeStatisticsMandatory" default="true" unit="bool" shortDescription="Het bepalen van code statistieken is verplicht." description="Wanneer de norm op 'true' staat dan moet er code statistieken bepaald zijn. Staat de norm op 'false' dan is het toegestaan dat er geen code statistieken bepaald worden." typeId="ruleCodeStatisticsMandatoryType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default rule.</annotation>
      </rule>
      <rule name="NrOfCodeStatisticsErrorsAllowed" default="0" unit="number" shortDescription="Code statistieken errors niet toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de er geen code statistieken errors zijn gerapporteerd. Staat de norm op een andere waarde dan is dat aantal aan code statistieken errors toegestaan." typeId="ruleNumberOfCodeStatisticsErrorsType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default rule.</annotation>
      </rule>
    </ruleSet>
    <ruleSet name="Compile">
      <rule name="CompileMandatory" default="true" unit="bool" shortDescription="Compilatie is verplicht." description="Wanneer de norm op 'true' staat is het verplicht om de compilatie stap uit te voeren. Staat de norm op 'false' dan is het toegestaan om geen compilatie uit te voeren." typeId="ruleCompileMandatoryType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default value.</annotation>
      </rule>
      <rule name="NrOfCompilerWarningsAllowed" default="0" unit="number" shortDescription="Compiler warnings niet toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de compiler geen warnings mag rapporteren. Staat de norm op een andere waarde dan is het toegestaan dat de compiler dat aantal warnings rapporteert." typeId="ruleNumberOfCompilerWarningsType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default rule.</annotation>
      </rule>
      <rule name="NrOfCompilerErrorsAllowed" default="0" unit="number" shortDescription="Compiler errors niet toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de compiler geen errors mag rapporteren. Staat de norm op een andere waarde dan is het toegestaan dat de compiler dat aantal errors rapporteert." typeId="ruleNumberOfCompilerErrorsType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default rule.</annotation>
      </rule>
      <rule name="CodeAnalysisCheckMandatory" default="true" unit="bool" shortDescription="FxCop controle is verplicht." description="Wanneer de norm op 'true' staat is het verplicht om FxCop controle uit te voeren. Staat de norm op 'false' dan is het toegestaan om de FxCop controle over te slaan." typeId="ruleCodeAnalysisCheckMandatoryType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default value.</annotation>
      </rule>
      <rule name="NrOfCodeAnalysisWarningsAllowed" default="0" unit="number" shortDescription="Code analysis warnings niet toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de compiler geen code analysis warnings mag rapporteren. Staat de norm op een andere waarde dan is het toegestaan dat de compiler dat aantal aan code analysis warnings rapporteert." typeId="ruleNumberOfCodeAnalysisWarningsType">
