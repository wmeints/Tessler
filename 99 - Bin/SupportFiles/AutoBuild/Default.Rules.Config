        <annotation>Default rule.</annotation>
      </rule>
      <rule name="NrOfCodeAnalysisErrorsAllowed" default="0" unit="number" shortDescription="Code analysis errors niet toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de compiler geen code analysis errors mag rapporteren. Staat de norm op een andere waarde dan is het toegestaan dat de compiler dat aantal aan code analysis errors rapporteert." typeId="ruleNumberOfCodeAnalysisErrorsType">
        <approvedBy>Info Support</approvedBy>
        <annotation>Default rule.</annotation>
      </rule>
      <rule name="NrOfExcludedMessagesWithoutJustificationAllowed" default="0" unit="number" shortDescription="Het aantal exluded messages zonder een justification toegestaan." description="Wanneer de norm op '0' staat, dan betekent dit dat de geen excluded messages mogen zijn zonder justification. Staat de norm op een andere waarde dan is het toegestaan dat er excluded messages zonder justification worden gerapporteerd." typeId="ruleNumberOfExcludedMessagesWithoutJustificationType">
