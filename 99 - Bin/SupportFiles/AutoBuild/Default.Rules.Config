  <ruleArguments>
    <ruleArgument name="XPathCompileNumberOfErrors" 
                  value="count(//results/message[@type='error' 
                         and not(@task='Microsoft.Build.Tasks.CodeAnalysis') 
                         and not(@task='CodeAnalysis') 
                         and not(@task='SqlStaticCodeAnalysisTask')
                         and not(. = preceding-sibling::message and @lineNumber = preceding-sibling::message/@lineNumber and @file = preceding-sibling::message/@file)
                         and not(@code='CS1591') and not(@code='CS1570') and not(@code='CS1571') and not(@code='CS1572') and not(@code='CS1573')
                         and not(@code='CS1574') and not(@code='CS1584') and not(@code='CS1587') and not(@code='CS1711') and not(@code='CS1723')
                          ])"/>
    <ruleArgument name="XPathCompileNumberOfWarnings"
                  value="count(//results/message[@type='warning' 
                         and not(@task='Microsoft.Build.Tasks.CodeAnalysis') 
                         and not(@task='CodeAnalysis') 
                         and not(@task='SqlStaticCodeAnalysisTask')
                         and not(contains(text(),&quot;The project configuration for project&quot;) and contains(text(), &quot;was not specified in the solution file for the solution configuration&quot;))
                         and not(contains(text(), &quot;Beginning retry 1 in 1000ms. The process cannot access the file &quot;))
                         and not(contains(text(), &quot;Beginning retry 2 in 1000ms. The process cannot access the file &quot;))
                         and not(@code='CS1591') and not(@code='CS1570') and not(@code='CS1571') and not(@code='CS1572') and not(@code='CS1573')
                         and not(@code='CS1574') and not(@code='CS1584') and not(@code='CS1587') and not(@code='CS1711') and not(@code='CS1723')
                         and not(. = preceding-sibling::message and @lineNumber = preceding-sibling::message/@lineNumber and @file = preceding-sibling::message/@file)
                         ])"/>
    <ruleArgument name="XPathCodeAnalysisNumberOfWarnings" value="count(//results/message[@type='warning' and (@task='Microsoft.Build.Tasks.CodeAnalysis' or @task='CodeAnalysis' or @task='SqlStaticCodeAnalysisTask') and not(. = preceding-sibling::message and @lineNumber = preceding-sibling::message/@lineNumber and @file = preceding-sibling::message/@file)])"/>
    <ruleArgument name="XPathCodeAnalysisNumberOfErrors" value="count(//results/message[@type='error' and (@task='Microsoft.Build.Tasks.CodeAnalysis' or @task='CodeAnalysis' or @task='SqlStaticCodeAnalysisTask') and not(. = preceding-sibling::message and @lineNumber = preceding-sibling::message/@lineNumber and @file = preceding-sibling::message/@file)])"/>
    <ruleArgument name="XPathCodeAnalysisCheck" value="count(//results/message[(@task='Microsoft.Build.Tasks.CodeAnalysis' or @task='CodeAnalysis' or @task='SqlStaticCodeAnalysisTask')]) > 0"/>
    <ruleArgument name="XPathNumberOfExcludedMessagesWithoutJustification" value="count (//Message) - count(//Message/Justification)"/>
    <ruleArgument name="XPathDocumentationNumberOfErrors" 
                  value="count(//results/message[@type='error' 
                        and not(@task='Microsoft.Build.Tasks.CodeAnalysis') 
                        and not(@task='CodeAnalysis')
                        and not(@task='SqlStaticCodeAnalysisTask')
                        and not(contains(text(), &quot;The project configuration for project&quot;) and contains(text(),&quot;was not specified in the solution file for the solution configuration&quot;)) 
                        and not(contains(text(),&quot;Found conflicts between different versions of the same dependent assembly.&quot;)) 
                        and (
                          @code='CS1591' or @code='CS1570' or @code='CS1571' or @code='CS1572' or @code='CS1573' or 
                          @code='CS1574' or @code='CS1584' or @code='CS1587' or @code='CS1711' or @code='CS1723')
                        ])"/>
    <ruleArgument name="XPathDocumentationNumberOfWarnings" 
                  value="count(//results/message[@type='warning' 
                        and not(@task='Microsoft.Build.Tasks.CodeAnalysis') 
                        and not(@task='CodeAnalysis')
                        and not(@task='SqlStaticCodeAnalysisTask')
                        and not(contains(text(),&quot;The project configuration for project&quot;) and contains(text(),&quot;was not specified in the solution file for the solution configuration&quot;)) 
                        and not(contains(text(),&quot;Found conflicts between different versions of the same dependent assembly.&quot;)) 
                        and (
                          @code='CS1591' or @code='CS1570' or @code='CS1571' or @code='CS1572' or @code='CS1573'or 
                          @code='CS1574'or @code='CS1584' or @code='CS1587'or @code='CS1711'or @code='CS1723')
                        ])"/>
    <ruleArgument name="XPathDeployForTestNumberOfErrors" value="count(//results/message[@type='error'])"/>
    <ruleArgument name="XPathDeployForTestNumberOfWarnings" value="count(//results/message[@type='warning'])"/>
    <ruleArgument name="XPathAutoTestNumberOfErrors" value="count(//ns:ResultSummary/ns:RunInfos/ns:RunInfo[@outcome='Error'])"/>
    <ruleArgument name="XPathAutoTestNumberOfLogErrors" value="count(//results/message[@type='error'])"/>
    <ruleArgument name="XPathAutoTestNumberOfLogWarnings" value="count(//results/message[@type='warning'])"/>
    <ruleArgument
         name="XPathAutoTestNumberOfWarnings"
         value="count(//ns:ResultSummary/ns:RunInfos/ns:RunInfo[@outcome='Warning' 
                and not(contains(./ns:Text/text(),'Instrumenting this image requires it to run as a 32-bit process.  The CLR header flags have been updated to reflect this.'))
                and not(contains(./ns:Text/text(),&quot;Test Run deployment issue: The assembly or module 'Microsoft.Internal.Mime' directly or indirectly referenced by the test container&quot;))
                and not(contains(./ns:Text/text(),&quot;Test Run deployment issue: The assembly or module 'Microsoft.WizardFrameworkVS' directly or indirectly referenced by the test container&quot;))
               ])"/>
    <ruleArgument name="XPathNumberOfTests" value="number(/ns:TestRun/ns:ResultSummary/ns:Counters/@total) - count(//ns:GenericTestResult/ns:Counters)"/>
    <ruleArgument name="XPathNumberOfExecutedTests" value="count(//ns:Results/*[not(ns:InnerResults)] | //ns:InnerResults/*[not(ns:InnerResults/*)])"/>
    <ruleArgument name="XPathNumberOfSucceedTests" value="count(//ns:Results/*[@outcome='Passed' and not(ns:InnerResults/*)] | //ns:InnerResults/*[@outcome='Passed' and not(ns:InnerResults/*)])"/>
    <ruleArgument name="XPathPercentageCodeCoverage" value="//CoverageDSPriv/Module"/>
    <ruleArgument name="XPathAutoDocNumberOfErrors" value="count(//results/message[@type='error'])"/>
    <ruleArgument name="XPathAutoDocNumberOfWarnings" value="count(//results/message[@type='warning'])"/>
    <ruleArgument name="XPathSandCastleResult" value="/shfbBuild/buildStep[last()]"/>
  </ruleArguments>
</ruleConfiguration>